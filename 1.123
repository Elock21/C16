#include <iostream>
using namespace std;
int prim(int x)
{
int d;
if(x<=1) return 0;
else for(d=2;d<=x/2;d++)
if(x%d==0) return 0;
return 1;
}
int main()
{
int v[256],i,n,x;
n=0;
while(x>=0 && n<256)
{
cin>>x;
v[n]=x;
n++;
}
n--;
cout<<"Afisare vector: ";
for(i=0;i<n;i++)
cout<<v[i]<<" ";
cout<<endl;
cout<<"Afisare vector din 2 in 2: ";
for(i=0;i<n;i=i+2)
cout<<v[i]<<" ";
cout<<endl;
cout<<"Afisare vector in ordine inversa: ";
for(i=n-1;i>=0;i--)
cout<<v[i]<<" ";
cout<<endl;
cout<<"Afisare nr prime: ";
for(i=0;i<n;i++)
if(prim(v[i])==1) cout<<v[i]<<" ";
cout<<endl;
int mx,mn;
for(i=0;i<n;i++)
if(i==0) {mn=v[i];mx=v[i];}
else if(v[i]>mx) mx=v[i];
else if(v[i]<mn) mn=v[i];
cout<<"Maximul: "<<mx<<" Minimul:"<<mn;
}
