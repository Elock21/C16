#include <iostream>
struct {
int n1,n2;
float m;
}v[10];
using namespace std;
int main()
{
int i,pmin,pmax;
float mmax,mmin;
for(i=0;i<10;i++)
{
cin>>v[i].n1;
cin>>v[i].n2;
v[i].m=(float)(v[i].n1+v[i].n2)/2;
cout<<v[i].m<<' ';
}
mmax=v[0].m; pmin=0;
mmin=v[0].m; pmax=0;
for(i=1;i<10;i++)
{
if(v[i].m>mmax) {mmax=v[i].m;pmax=i;}
if(v[i].m<mmin) {mmin=v[i].m;pmin=i;}
}
cout<<"Media maxima: "<<mmax<<" Pozitia:"<<pmax<<endl;
cout<<"Media minima: "<<mmin<<" Pozitia:"<<pmin<<endl;
}
